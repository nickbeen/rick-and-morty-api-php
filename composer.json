{
	"name": "nickbeen/rick-and-morty-api-php",
	"description": "The Rick And Morty API PHP Client",
	"type": "library",
	"keywords": [
		"api",
		"php",
		"rick and morty"
	],
	"homepage": "https://github.com/nickbeen/rick-and-morty-api-php",
	"license": "MIT",
	"authors": [
		{
			"name": "Nick Been",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^8.1",
		"ext-json": "*",
		"guzzlehttp/guzzle": "^7.4",
		"myclabs/php-enum": "^1.8",
		"netresearch/jsonmapper": "^v4.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^10.1"
	},
	"autoload": {
		"psr-4": {
			"NickBeen\\RickAndMortyPhpApi\\": "src"
		}
	},
	"autoload-dev": {
        "psr-4": {
            "NickBeen\\RickAndMortyPhpApi\\Tests\\": "tests"
        }
    },
	"scripts": {
		"test": "vendor/bin/phpunit"
	},
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
